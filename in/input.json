{
  "eobs": {
    "parameters": {
      "areas_id_column": "gauge_id",
      "variables": "air_pressure_sea_level",
      "skip_exist": false
    },
    "data": {
      "areas": "/in/catchments/catchments.gpkg",
      "eobs_stgrid_air_pressure_sea_level": "/in/eobs/*.nc"
    }
  },
  "radklim_rw": {
    "parameters": {
      "areas_id_column": "gauge_id",
      "skip_exist": false,
      "batch_size": 40
    },
    "data": {
      "areas": "/in/catchments/catchments.gpkg",
      "radklim_rw_stgrid": "/in/radklim_rw/2004/*.nc"
    }
  },
  "hostrada": {
    "parameters": {
      "areas_id_column": "gauge_id",
      "variables": "cloud_cover",
      "save_nc": false,
      "save_csv": true,
      "skip_exist": false,
      "batch_size": 40
    },
    "data": {
      "areas": "/in/catchments/catchments.gpkg",
      "hostrada_stgrid_cloud_cover": "/in/hostrada/cloud_cover/1999/*.nc"
    }
  }
